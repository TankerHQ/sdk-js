{
  "name": "tanker",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build:all": "yarn build:test-utils && yarn build:global-this && yarn build:crypto && yarn build:emails && yarn build:errors && yarn build:file-ponyfill && yarn build:file-reader && yarn build:http-utils && yarn build:types && yarn build:streams && yarn build:core && yarn build:datastores && yarn build:client-browser && yarn build:client-node && yarn build:verification-ui && yarn build:fake-authentication && yarn build:filekit",
    "build:ts": "yarn build:test-utils && yarn build:global-this && yarn build:errors && yarn build:file-ponyfill && yarn build:file-reader && yarn build:http-utils && yarn build:crypto && yarn build:types && yarn build:streams && yarn build:datastores && yarn build:core && yarn build:client-browser && yarn build:client-node && yarn build:fake-authentication && yarn build:verification-ui && yarn build:filekit && yarn build:functional-tests",
    "build:client-browser-base": "rm -rf packages/client-browser/dist && tsc --build packages/client-browser/config/tsconfig.json",
    "build:client-browser-umd": "webpack --config packages/client-browser/config/webpack.config.js",
    "build:client-browser": "yarn build:client-browser-base && yarn build:client-browser-umd",
    "build:client-node": "rm -rf packages/client-node/dist && tsc --build packages/client-node/config/tsconfig.json",
    "build:core": "rm -rf packages/core/dist && tsc --build packages/core/config/tsconfig.json",
    "build:crypto": "rm -rf packages/crypto/dist && tsc --build packages/crypto/config/tsconfig.json",
    "build:datastores": "rm -rf packages/datastore/*/dist && tsc --build packages/datastore/config/tsconfig.json",
    "start:emails": "cd packages/emails; yarn start",
    "build:emails": "cd packages/emails; yarn build",
    "build:errors": "rm -rf packages/errors/dist && tsc --build packages/errors/config/tsconfig.json",
    "build:fake-authentication": "rm -rf packages/fake-authentication/dist && tsc --build packages/fake-authentication/config/tsconfig.json",
    "build:file-ponyfill": "rm -rf packages/file-ponyfill/dist && tsc --build packages/file-ponyfill/config/tsconfig.json",
    "build:file-reader": "rm -rf packages/file-reader/dist && tsc --build packages/file-reader/config/tsconfig.json",
    "build:filekit": "rm -rf packages/filekit/dist && tsc --build packages/filekit/config/tsconfig.json",
    "build:global-this": "rm -rf packages/global-this/dist && tsc --build packages/global-this/config/tsconfig.json",
    "build:http-utils": "rm -rf packages/http-utils/dist && tsc --build packages/http-utils/config/tsconfig.json",
    "build:streams": "rm -rf packages/stream/*/dist && tsc --build packages/stream/config/tsconfig.json",
    "build:test-utils": "rm -rf packages/test-utils/dist && tsc --build packages/test-utils/config/tsconfig.json",
    "build:types": "rm -rf packages/types/dist && tsc --build packages/types/config/tsconfig.json",
    "build:verification-ui-base": "rm -rf packages/verification-ui/dist && tsc --build packages/verification-ui/config/tsconfig.json",
    "build:verification-ui-umd": "webpack --config packages/verification-ui/config/webpack.config.production.js",
    "build:verification-ui": "yarn build:verification-ui-base && yarn build:verification-ui-umd",
    "build:functional-tests": "rm -rf packages/functional-tests/dist && tsc --build packages/functional-tests/config/tsconfig.json",
    "start:verification-ui": "webpack serve --hot --config packages/verification-ui/config/webpack.config.development.js",
    "lint:js": "eslint \"packages/**/*.js\"",
    "lint:ts": "eslint --no-eslintrc --config \".eslintrc.typescript.yml\"",
    "lint:ts:all": "yarn lint:ts \"packages/**/*.ts{,x}\"",
    "lint:compat": "yarn eslint --no-ignore --no-eslintrc --config .eslintrc.compat.yml",
    "lint:compat:all": "yarn lint:compat \"packages/**/dist/**/*.js\"",
    "lint:flow": "flow check",
    "test": "mocha \"packages/**/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:core": "mocha \"packages/core/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:crypto": "mocha \"packages/crypto/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-base": "mocha \"packages/datastore/base/src/**/*.spec{,.node}.ts\" --exit  --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-pouchdb-memory": "mocha \"packages/datastore/pouchdb-memory/src/**/*.spec{,.node}.ts\" --exit  --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-pouchdb-node": "mocha \"packages/datastore/pouchdb-node/src/**/*.spec{,.node}.ts\" --exit  --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:errors": "mocha \"packages/errors/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:global-this": "mocha \"packages/global-this/**/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:http-utils": "mocha \"packages/http-utils/**/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:streams": "mocha \"packages/stream/**/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test:types": "mocha \"packages/types/src/**/*.spec{,.node}.ts\" --exit --require ts-node/register --recursive --full-trace --trace-warnings --reporter=spec",
    "karma": "karma start config/karma/karma.config.ci.js",
    "debug": "karma start config/karma/karma.config.debug.js",
    "debug:core": "karma start packages/core/config/karma.config.debug.js",
    "debug:crypto": "karma start packages/crypto/config/karma.config.debug.js",
    "debug:errors": "karma start packages/errors/config/karma.config.debug.js",
    "debug:streams": "karma start packages/stream/config/karma.config.debug.js",
    "debug:verification-ui": "karma start packages/verification-ui/config/karma.config.debug.js",
    "benchmark": "karma start config/karma/karma.config.benchmark.js",
    "proof": "yarn lint:js && yarn lint:types && yarn test",
    "coverage": "nyc yarn test"
  },
  "dependencies": {
    "@babel/core": "7.12.16",
    "@babel/eslint-parser": "7.12.16",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-transform-flow-strip-types": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "7.12.13",
    "@rollup/plugin-babel": "5.3.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/file-saver": "^2.0.3",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/mocha": "^8.2.3",
    "@types/pouchdb-adapter-leveldb": "^6.1.3",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "@types/pouchdb-core": "^7.0.8",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/react-motion": "^0.0.31",
    "@types/readable-stream": "^2.3.11",
    "@types/sinon": "^10.0.2",
    "@types/styled-components": "^5.1.14",
    "@types/tmp": "^0.2.1",
    "@types/uuid": "^8.3.1",
    "@types/varint": "^6.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "5.7.1",
    "codecov": "3.8.1",
    "colorette": "1.2.1",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-compat": "^3.11.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "eslint-plugin-react": "7.22.0",
    "flow-bin": "0.131.0",
    "fs-extra": "9.1.0",
    "karma": "6.1.1",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-edgium-launcher": "https://github.com/matracey/karma-edgium-launcher.git#704328a9c0589c342121b3db8838f6d3ca412702",
    "karma-firefox-launcher": "2.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-opera-launcher": "1.0.0",
    "karma-safarinative-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "make-error": "^1.3.6",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "process": "^0.11.10",
    "rollup": "2.41.2",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-local-resolve": "1.0.7",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "util": "^0.12.3",
    "webpack": "5.36.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "workspaces": [
    "packages/*",
    "packages/datastore/*",
    "packages/stream/*"
  ],
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not ie < 11",
    "not dead",
    "not node < 10"
  ],
  "nyc": {
    "exclude": [
      "config",
      "packages/*/config",
      "packages/**/*.spec.js",
      "packages/**/*.spec.node.js",
      "packages/**/*.spec.web.js",
      "packages/functional-tests",
      "packages/**/src/__tests__",
      "packages/**/tests"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".ts"
    ],
    "sourceMap": true
  },
  "resolutions": {
    "browserslist": "^4.16.6",
    "eslint-import-resolver-node": "0.3.4",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^2.8.9",
    "lodash": "^4.17.21",
    "multicast-dns": "^7.2.3",
    "node-fetch": "^2.6.1",
    "url-parse": "^1.5.3",
    "ws": "^7.4.6"
  }
}
