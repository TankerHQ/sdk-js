{
  "name": "tanker",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build:all": "yarn build:global-this && yarn build:crypto && yarn build:emails && yarn build:errors && yarn build:file-ponyfill && yarn build:file-reader && yarn build:http-utils && yarn build:types && yarn build:streams && yarn build:core && yarn build:datastores && yarn build:client-browser && yarn build:client-node && yarn build:verification-ui && yarn build:fake-authentication && yarn build:filekit",
    "build:client-browser-base": "rollup --config packages/client-browser/config/rollup.js",
    "build:client-browser-umd": "webpack --config packages/client-browser/config/webpack.config.js",
    "build:client-browser": "yarn build:client-browser-base && yarn build:client-browser-umd",
    "build:client-node": "rollup --config packages/client-node/config/rollup.js",
    "build:core": "rollup --config packages/core/config/rollup.js",
    "build:crypto": "rollup --config packages/crypto/config/rollup.js",
    "build:datastores": "rollup --config packages/datastore/config/rollup.js",
    "start:emails": "cd packages/emails; yarn start",
    "build:emails": "cd packages/emails; yarn build",
    "build:errors": "rollup --config packages/errors/config/rollup.js",
    "build:fake-authentication": "rollup --config packages/fake-authentication/config/rollup.js",
    "build:file-ponyfill": "rollup --config packages/file-ponyfill/config/rollup.js",
    "build:file-reader": "rollup --config packages/file-reader/config/rollup.js",
    "build:filekit": "rollup --config packages/filekit/config/rollup.js",
    "build:global-this": "rollup --config packages/global-this/config/rollup.js",
    "build:http-utils": "rollup --config packages/http-utils/config/rollup.js",
    "build:streams": "rollup --config packages/stream/config/rollup.js",
    "build:types": "rollup --config packages/types/config/rollup.js",
    "build:verification-ui-base": "rollup --config packages/verification-ui/config/rollup.js",
    "build:verification-ui-umd": "webpack --config packages/verification-ui/config/webpack.config.production.js",
    "build:verification-ui": "yarn build:verification-ui-base && yarn build:verification-ui-umd",
    "start:verification-ui": "webpack serve -w --hot --config packages/verification-ui/config/webpack.config.development.js",
    "lint:js": "eslint \"packages/**/*.js\"",
    "lint:flow": "flow check",
    "test": "mocha \"packages/**/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:core": "mocha \"packages/core/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:crypto": "mocha \"packages/crypto/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-base": "mocha \"packages/datastore/base/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-pouchdb-memory": "mocha \"packages/datastore/pouchdb-memory/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:datastore-pouchdb-node": "mocha \"packages/datastore/pouchdb-node/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:errors": "mocha \"packages/errors/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:global-this": "mocha \"packages/global-this/**/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:http-utils": "mocha \"packages/http-utils/**/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:streams": "mocha \"packages/stream/**/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "test:types": "mocha \"packages/types/src/**/*.spec{,.node}.js\" --exit --require ./config/babel.require-hook.js --recursive --full-trace --trace-warnings --reporter=spec",
    "karma": "karma start config/karma/karma.config.ci.js",
    "debug": "karma start config/karma/karma.config.debug.js",
    "debug:core": "karma start packages/core/config/karma.config.debug.js",
    "debug:crypto": "karma start packages/crypto/config/karma.config.debug.js",
    "debug:errors": "karma start packages/errors/config/karma.config.debug.js",
    "debug:streams": "karma start packages/stream/config/karma.config.debug.js",
    "debug:verification-ui": "karma start packages/verification-ui/config/karma.config.debug.js",
    "benchmark": "karma start config/karma/karma.config.benchmark.js",
    "proof": "yarn lint:js && yarn lint:flow && yarn test",
    "coverage": "nyc --reporter=lcov --reporter=text-summary yarn test"
  },
  "dependencies": {
    "@babel/core": "7.12.16",
    "@babel/eslint-parser": "7.12.16",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-transform-flow-strip-types": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@babel/register": "7.12.13",
    "@rollup/plugin-babel": "5.3.0",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "6.0.0",
    "buffer": "5.7.1",
    "codecov": "3.8.1",
    "colorette": "1.2.1",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "eslint-plugin-react": "7.22.0",
    "flow-bin": "0.131.0",
    "fs-extra": "9.1.0",
    "karma": "6.1.1",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-edgium-launcher": "https://github.com/matracey/karma-edgium-launcher.git#704328a9c0589c342121b3db8838f6d3ca412702",
    "karma-firefox-launcher": "2.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-opera-launcher": "1.0.0",
    "karma-safarinative-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "process": "^0.11.10",
    "rollup": "2.41.2",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-local-resolve": "1.0.7",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "util": "^0.12.3",
    "webpack": "5.36.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "workspaces": [
    "packages/*",
    "packages/datastore/*",
    "packages/stream/*"
  ],
  "nyc": {
    "exclude": [
      "config",
      "packages/*/config",
      "packages/**/*.spec.js",
      "packages/**/*.spec.node.js",
      "packages/**/*.spec.web.js",
      "packages/functional-tests",
      "packages/**/src/__tests__",
      "packages/**/tests"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "resolutions": {
    "browserslist": "^4.16.6",
    "eslint-import-resolver-node": "0.3.4",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^2.8.9",
    "lodash": "^4.17.21",
    "multicast-dns": "^7.2.3",
    "node-fetch": "^2.6.1",
    "url-parse": "^1.5.3",
    "ws": "^7.4.6"
  }
}
