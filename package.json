{
  "name": "tanker",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build:all": "yarn build:ts && yarn build:emails",
    "build:ts": "yarn build:test-utils && yarn build:global-this && yarn build:errors && yarn build:file-ponyfill && yarn build:file-reader && yarn build:http-utils && yarn build:crypto && yarn build:types && yarn build:streams && yarn build:datastores && yarn build:core && yarn build:client-browser && yarn build:client-node && yarn build:fake-authentication && yarn build:verification-ui && yarn build:filekit && yarn build:functional-tests",
    "build:client-browser-base": "rimraf packages/client-browser/dist && tsc --build packages/client-browser/config",
    "build:client-browser-umd": "webpack --config packages/client-browser/config/webpack.config.js",
    "build:client-browser": "yarn build:client-browser-base && yarn build:client-browser-umd",
    "build:client-node": "rimraf packages/client-node/dist && tsc --build packages/client-node/config",
    "build:core": "rimraf packages/core/dist && tsc --build packages/core/config",
    "build:crypto": "rimraf packages/crypto/dist && tsc --build packages/crypto/config",
    "build:datastores": "rimraf packages/datastore/*/dist && tsc --build packages/datastore/config",
    "start:emails": "cd packages/emails; yarn start",
    "build:emails": "cd packages/emails; yarn build",
    "build:errors": "rimraf packages/errors/dist && tsc --build packages/errors/config",
    "build:fake-authentication": "rimraf packages/fake-authentication/dist && tsc --build packages/fake-authentication/config",
    "build:file-ponyfill": "rimraf packages/file-ponyfill/dist && tsc --build packages/file-ponyfill/config",
    "build:file-reader": "rimraf packages/file-reader/dist && tsc --build packages/file-reader/config",
    "build:filekit": "rimraf packages/filekit/dist && tsc --build packages/filekit/config",
    "build:global-this": "rimraf packages/global-this/dist && tsc --build packages/global-this/config",
    "build:http-utils": "rimraf packages/http-utils/dist && tsc --build packages/http-utils/config",
    "build:streams": "rimraf packages/stream/*/dist && tsc --build packages/stream/config",
    "build:test-utils": "rimraf packages/test-utils/dist && tsc --build packages/test-utils/config",
    "build:types": "rimraf packages/types/dist && tsc --build packages/types/config",
    "build:verification-ui-base": "rimraf packages/verification-ui/dist && tsc --build packages/verification-ui/config",
    "build:verification-ui-umd": "webpack --config packages/verification-ui/config/webpack.config.production.js",
    "build:verification-ui": "yarn build:verification-ui-base && yarn build:verification-ui-umd",
    "build:functional-tests": "rimraf packages/functional-tests/dist && tsc --build packages/functional-tests/config",
    "start:verification-ui": "webpack serve --hot --config packages/verification-ui/config/webpack.config.development.js",
    "lint:js": "eslint \"config/**/*.js\" \"packages/**/*.js\"",
    "lint:ts": "eslint",
    "lint:ts:all": "yarn lint:ts \"packages/**/*.ts{,x}\"",
    "lint:compat": "yarn eslint --no-ignore --no-eslintrc --config .eslintrc.compat.yml",
    "lint:compat:all": "yarn lint:compat \"packages/**/dist/**/*.js\"",
    "_test": "mocha --exit --require ts-node/register --require tsconfig-paths/register --recursive --full-trace --trace-warnings --reporter=spec",
    "test": "yarn _test \"packages/**/src/**/*.spec{,.node}.ts\"",
    "test:core": "yarn _test \"packages/core/src/**/*.spec{,.node}.ts\"",
    "test:crypto": "yarn _test \"packages/crypto/src/**/*.spec{,.node}.ts\"",
    "test:datastore-base": "yarn _test \"packages/datastore/base/src/**/*.spec{,.node}.ts\"",
    "test:datastore-pouchdb-memory": "yarn _test \"packages/datastore/pouchdb-memory/src/**/*.spec{,.node}.ts\"",
    "test:datastore-pouchdb-node": "yarn _test \"packages/datastore/pouchdb-node/src/**/*.spec{,.node}.ts\"",
    "test:errors": "yarn _test \"packages/errors/src/**/*.spec{,.node}.ts\"",
    "test:global-this": "yarn _test \"packages/global-this/**/src/**/*.spec{,.node}.ts\"",
    "test:http-utils": "yarn _test \"packages/http-utils/**/src/**/*.spec{,.node}.ts\"",
    "test:streams": "yarn _test \"packages/stream/**/src/**/*.spec{,.node}.ts\"",
    "test:types": "yarn _test \"packages/types/src/**/*.spec{,.node}.ts\"",
    "karma": "karma start config/karma/karma.config.ci.js",
    "debug": "karma start config/karma/karma.config.debug.js",
    "debug:core": "karma start packages/core/config/karma.config.debug.js",
    "debug:crypto": "karma start packages/crypto/config/karma.config.debug.js",
    "debug:errors": "karma start packages/errors/config/karma.config.debug.js",
    "debug:streams": "karma start packages/stream/config/karma.config.debug.js",
    "debug:verification-ui": "karma start packages/verification-ui/config/karma.config.debug.js",
    "benchmark": "karma start config/karma/karma.config.benchmark.js",
    "proof": "poetry install && poetry run python run-ci.py lint && yarn test",
    "coverage": "nyc yarn test"
  },
  "dependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.3",
    "@types/sinon": "^10.0.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "buffer": "5.7.1",
    "codecov": "3.8.1",
    "colorette": "1.2.1",
    "core-js": "^3.18.3",
    "core-js-pure": "^3.18.3",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-compat": "^3.11.1",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "eslint-plugin-react": "7.22.0",
    "fs-extra": "9.1.0",
    "karma": "6.3.11",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-edgium-launcher": "https://github.com/matracey/karma-edgium-launcher.git#704328a9c0589c342121b3db8838f6d3ca412702",
    "karma-firefox-launcher": "2.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-opera-launcher": "1.0.0",
    "karma-safarinative-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "make-error": "^1.3.6",
    "mocha": "^9.1.2",
    "nyc": "15.1.0",
    "process": "^0.11.10",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2",
    "util": "^0.12.3",
    "webpack": "5.64.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "workspaces": [
    "packages/*",
    "packages/datastore/*",
    "packages/stream/*"
  ],
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not ie < 11",
    "not dead",
    "not node < 10"
  ],
  "nyc": {
    "exclude": [
      "config",
      "packages/*/config",
      "packages/**/*.spec.ts",
      "packages/**/*.spec.node.ts",
      "packages/**/*.spec.web.ts",
      "packages/functional-tests",
      "packages/**/src/**/__tests__",
      "packages/**/tests"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "sourceMap": true
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "browserslist": "^4.16.6",
    "eslint-import-resolver-node": "0.3.4",
    "follow-redirects": "^1.14.7",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^2.8.9",
    "lodash": "^4.17.21",
    "multicast-dns": "^7.2.3",
    "node-fetch": "^2.6.1",
    "node-forge": "^1.0.0",
    "url-parse": "^1.5.3",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
